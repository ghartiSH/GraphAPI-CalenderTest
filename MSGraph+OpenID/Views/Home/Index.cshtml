
@model List<Calendar>

@{
    ViewData["Title"] = "Home Page";
    var profileImage = @ViewData["ProfileImage"];
    var calender = @ViewData["CalenderData"];
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div>
    <h5>Your Display Name : <span>@ViewData["UserName"]</span></h5>
</div>

<div>
    <img src="data:image/jpeg;base64, @profileImage" 
                    class="user-photo-small nav-profile-photo rounded-circle align-self-center mr-2" />
</div>

<div>
    <h2>List of Events: </h2>
    @foreach(var item in Model)
    {
        <div class="card">
            <h5>Subject: @item.Subject </h5>
            <h5>Date:  @item.Date</h5>
        </div>
        <br />
    }
</div>
